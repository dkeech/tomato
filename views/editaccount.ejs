<%- include("partials/header") %>
<div class="col-md-4 signIn">
    <form>
        <div class="form-group">
          <label for="exampleInputEmail1">Change Email</label>
          <input type="email" name = "email" class="form-control" id="changeEmail" aria-describedby="emailHelp" placeholder="[email]">
          
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Change Password</label>
          <input type="password" name = "password" class="form-control" id="password" placeholder="*******">
        </div>
        
        <button type="button" id="submit_update" class="btn btn-primary">Update</button>
        
    </form>
</div>

<script src="/update_account.js"></script>    
